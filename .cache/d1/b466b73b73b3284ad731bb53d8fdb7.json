1 ? store.currentPage - 1 : 1\r\n  );\r\n  template = template.replace(\r\n    \"{{__next_page__}}\",\r\n    newsFeed[store.currentPage * 10] ? store.currentPage + 1 : store.currentPage\r\n  );\r\n  container.innerHTML = template;\r\n}\r\n\r\nfunction newsDetail() {\r\n  const id = location.hash.substr(7); //첫번쨰부터 끝까지의 문자열만 반환(#을 제거한 숫자값만 얻을 수 있다.)\r\n\r\n  const newsContent = getData(CONTENT_URL.replace(\"@id\", id));\r\n  container.innerHTML = `\r\n    <h1>${newsContent.title} </h1>\r\n    <div>\r\n      <a href=\"#/page/${store.currentPage}\">목록으로</a>\r\n    </div>\r\n   `;\r\n}\r\nfunction router() {\r\n  const routePath = location.hash;\r\n  if (routePath === \"\") {\r\n    newsFeed();\r\n  } else if (routePath.indexOf(\"#/page/\") >= 0) {\r\n    store.currentPage = Number(routePath.substr(7));\r\n    newsFeed();\r\n  } else {\r\n    newsDetail();\r\n  }\r\n}\r\nwindow.addEventListener(\"hashchange\", router);\r\nrouter();\r\n"},"lineCount":null}},"error":null,"hash":"b353533467b9afe3daf6887239484166","cacheData":{"env":{}}}